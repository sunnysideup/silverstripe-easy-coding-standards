#!/bin/bash

echonice() {
   echo -e "\033[0;33m"$1" \033[0m";
}
echogood() {
   echo -e "\033[0;32m"$1" \033[0m";
}
echobad() {
   echo -e "\033[1;32;41m"$1" \033[0m";
}

echoline() {
    echonice '----------------------------------------';
}

echoend() {
    echoline;
    echoline;
    echoline;
    echonice 'END'
    echo "";
    echo "";
}
